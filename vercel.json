{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/sub/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
